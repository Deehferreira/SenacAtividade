@model List<Usuario>
@{
    ViewData["Title"] = "Listagem de Usuário";
}
<h2>Listagem de Usuário</h2>
<table class="table table-bordered table-stripped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nome</th>
            <th>Login</th>
            <th>Senha</th>
            <th>Data de nascimento</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Usuario u in Model)
        {
            <tr>
                <td>@u.Id</td>
                <td>@u.Nome</td>
                <td>@u.Login</td>
                <td>@u.Senha</td>
                <td>@u.DataNascimento</td>
            </tr>
        }
    </tbody>
</table>